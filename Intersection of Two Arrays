class Solution {
    public int[] intersection(int[] nums1, int[] nums2) {
     Set<Integer>set1=new HashSet<>();
for(int i=0;i<nums1.length;i++){
    set1.add(nums1[i]);
}
int[] res=new int[set1.size()];
int c=0;
for(int j=0;j<nums2.length;j++){
    if(set1.contains(nums2[j])){
       res[c++]=nums2[j]; 
       set1.remove(nums2[j]);
    }
}
int temp[]=new int[c];
int ind=0;
for(int j=0;j<c;j++){
   temp[ind++]=res[j];
}
     return temp;
    }
}
