class Solution {
    public static int findMin(int[] nums) {
        int left=0,right=nums.length-1;
       int min=Integer.MAX_VALUE;
        while(left<=right){
        int mid=(left+right)/2;
        if(nums[mid]<nums[right]){
             min=Math.min(nums[mid],min);
             right=mid-1;
        }
        else {
              min=Math.min(nums[mid],min);
             left=mid+1;

        }
       } 
       return min;
    }
}
