/**
 * Definition for singly-linked list.
 * class ListNode {
 *     int val;
 *     ListNode next;
 *     ListNode(int x) {
 *         val = x;
 *         next = null;
 *     }
 * }
 */
public class Solution {
    public ListNode detectCycle(ListNode head) {
        if(head==null){
            return head;
        }
        ListNode slow=head;
        ListNode fast=head;
        ListNode temp=null;
        while(fast!=null && fast.next!=null){
             slow=slow.next;
            fast=fast.next.next;
            if(slow==fast){
                temp=slow;
                slow=head;
                break;
            }
        }
        if(temp==fast){
            while(slow!=fast && fast!=null){
                slow=slow.next;
                fast=fast.next;
            }
            return fast;
        }
        return null;
    }
}
