class Solution {
    public int maxProduct(int[] nums) {
     int max=nums[0];
     int left=0, right=0;
     int n=nums.length;
     for(int i=0;i<n;i++){
        left=(left==0?1:left)*nums[i];
        right=(right==0?1:right)*nums[n-i-1];
        max=Math.max(max,Math.max(left,right));
     }
     return max;
    }
}
